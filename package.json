{
  "name": "wp-graphql-composer",
  "version": "0.4.0",
  "description": "A light-weight React component library for developing React apps served by WPGraphQL servers",
  "keywords": [
    "wp-graphql",
    "wordpress",
    "react",
    "apollo",
    "graphql",
    "react-apollo",
    "recompose"
  ],
  "author": {
    "name": "kidunot89",
    "url": "https://axistaylor.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kidunot89/wp-graphql-composer.git"
  },
  "bugs": {
    "url": "https://github.com/kidunot89/wp-graphql-composer/issues"
  },
  "bin": {
    "wpg-intro": "./bin/introspection-fetch.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "moment": "^2.22.2",
    "react-html-parser": "^2.0.2"
  },
  "peerDependencies": {
    "apollo-boost": "^0.1.20",
    "chalk": "^2.4.1",
    "graphql": "^14.0.2",
    "node-fetch": "^2.3.0",
    "react": "^16.6.3",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "apollo-boost": "^0.1.20",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-react-app": "^6.1.0",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "graphql": "^14.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-fetch": "^2.3.0",
    "node-sass": "^7.0.0",
    "postcss-scss": "^2.0.0",
    "react": "^16.6.3",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.6.3",
    "react-testing-library": "^5.2.3",
    "recompose": "^0.30.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "proxy": "http://wpgraphql.example:8000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(?:scss|css|sass)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepublish": "rm -rf ./dist && npm run build",
    "tester": "jest test --no-cache --watch",
    "test": "jest test --no-cache",
    "coveralls": "jest test --coverage --coverageReporters=text-lcov | coveralls"
  }
}
